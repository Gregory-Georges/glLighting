#version 310 es
precision highp float;



//Variables from vertex
in vec2 vTexPos;
in vec4 vColors;

//Color uniforms
uniform vec4 objectColor;
uniform vec4 lightColor;

//Texture
uniform sampler2D tex;

//Output color
out vec4 outColors;



void main()
{
    outColors = texture(tex, vTexPos) * objectColor * lightColor * vColors;
}
